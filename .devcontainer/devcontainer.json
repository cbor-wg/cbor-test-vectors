{
  "name": "Node.js",
  "image": "mcr.microsoft.com/devcontainers/javascript-node:4.0.6-24-trixie",
  "customizations": {
    "vscode": {
      "extensions": [
        "editorconfig.editorconfig",
        "max-schlesinger.cbor-edn-editor",
        "vitest.explorer"
